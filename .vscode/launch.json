  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno Run Debug",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--allow-all", "--inspect-brk", "app.ts"],
      "port": 9229,
      "outputCapture": "std"
    },
    {
      "name": "Deno Tests Debug",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["test", "--inspect-brk", "--allow-all"],
      "port": 9229
    }
  ]
}
